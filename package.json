{
  "name": "tabnews-clone",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "services:up": "docker compose --file infra/compose.yaml up -d",
    "services:down": "docker compose --file infra/compose.yaml down",
    "services:stop": "docker compose --file infra/compose.yaml stop",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "up:docker": "docker compose --file infra/compose.yaml up",
    "down:docker": "docker compose --file infra/compose.yaml down",
    "migrate:create": "node-pg-migrate --migrations-dir infra/migrations create",
    "migrate:up": "node-pg-migrate --migrations-dir infra/migrations up"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.4",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.0.3"
  }
}
